    <script>
        $('#in_search').keypress(function(e) {
            if(e.which == 13) {
                updatetc();
            }
        });
        function updatetc(){
            var f=$('#in_search').val();
            if (f=='Search by keyword')f='';

            var b=$('#in_con').val();
            var c=$('#in_cat').val();

            if ( $('#in_due').prop("checked") ){
                get_url('/topconf/?ajax=1&k='+escape(f)+'&due=1&con='+escape(b)+'&cat='+escape(c),'ajax_content');
            }else{
                get_url('/topconf/?ajax=1&k='+escape(f)+'&due=0&con='+escape(b)+'&cat='+escape(c),'ajax_content');
            }

        }
        function updatec(v){
            get_url('/topconf/?addc=1'+'&v='+escape(v),'div_msg');
        }
        function updatecr(v){
            get_url('/topconf/?removec=1'+'&v='+escape(v),'div_msg');
        }

        document.querySelector('.read-more-click').onclick = function() {
            document.querySelector('.read-more-hidden').classList.remove('read-more-hidden');
            this.style.display = 'none';
        }
    </script>

