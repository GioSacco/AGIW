<script>
$('#in_search').keypress(function(e) {
    if(e.which == 13) {
        updatej();
    }
});
function updatej(){
var f=$('#in_search').val();
if (f=='Search by keyword')f='';

var b=$('#in_pub').val();
var c=$('#in_cat').val();

get_url('/journals/?ajax=1&k='+escape(f)+'&pub='+escape(b)+'&cat='+escape(c),'ajax_content');
}
function updatec(v){
get_url('/journals/?addc=1'+'&v='+escape(v),'div_msg');
}
function updatecr(v){
get_url('/journals/?removec=1'+'&v='+escape(v),'div_msg');
}
document.querySelector('.read-more-click').onclick = function() {
        document.querySelector('.read-more-hidden').classList.remove('read-more-hidden');
        this.style.display = 'none';
}


</script>
