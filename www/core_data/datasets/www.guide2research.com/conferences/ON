<script>
$('#in_search').keypress(function(e) {
    if(e.which == 13) {
        updatetc();
    }
});
function updatetc(){
var f=$('#in_search').val();
if (f=='Search by keyword')f='';

var p=$('#in_pub').val();
var b=$('#in_con').val();
var c=$('#in_cat').val();

var d=$('#in_date').val();
get_url('/conferences/?ajax=1&k='+escape(f)+'&dd='+d+'&pub='+p+'&con='+escape(b)+'&cat='+escape(c),'ajax_content');

}
function updatec(v){
get_url('/conferences/?addc=1'+'&v='+escape(v),'div_msg');
}
function updatecr(v){
get_url('/conferences/?removec=1'+'&v='+escape(v),'div_msg');
}


</script>

